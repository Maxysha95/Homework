class Node:

    def __init__(self, elem, nextnode):
        self.elem = elementsself.nextnode = nextnode

class LinkedStack:
    def __init__(self):
        self.emprynode = Node(None, None)
        self.top = self.emptyNode

    def push(self, elem):
        node = Node(elem,self.top)
        self.top = node



emptyNode = Node(None, None)
node0 = Node(0**3, emptyNode)
node1 = Node(1**3, node0)
